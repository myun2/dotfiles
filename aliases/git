alias add="git add"
alias add.="git add ."
alias ame="git commit --amend"
alias co="git commit"
alias diff="git diff --cached"
alias deltag="git tag -d"
alias h="git reset --hard"
alias log2="git log2"
alias pop="git stash pop"
alias pull="git pull"
alias push="git push"
alias push-f="git push -f"
alias reset="git reset"
alias reflog="git reflog"
alias s="git show"
alias stat="git status"
alias stash="git stash"
alias tag="git tag"
function totag {
  br=`curbr`
  sw develop
  git tag $br
  git branch -D $br
}
function untag {
  git checkout $1
  brsw $1
  deltag $1
}
